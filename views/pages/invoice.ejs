<body>

<style>
    body{margin-top:20px;
    background-color:#eee;
    
}

.card {
    box-shadow: 0 20px 27px 0 rgb(0 0 0 / 5%);
}
.card {
    position: relative;
    display: flex;
    flex-direction: column;
    min-width: 0;
    word-wrap: break-word;
    background-color: #fff;
    background-clip: border-box;
    border: 0 solid rgba(0,0,0,.125);
    border-radius: 1rem;
}
p{
    color: #000;
    font-size: 15px;
    font-weight: 500;
}
h5{
    font-size: 15px;
}

</style>

    <button style="float: right;"><a href="/profile/myorders/orderDetails/<%= orderDetails._id %>">Back</a></li></button>

<br><br>

    <div class="container">
        <div class="row">
                <div class="col-lg-12">
                    <div class="card">  
                        <div class="card-body">
                            <div class="invoice-title">
                                <h5 class="float-end font-size-15">Invoice <span class="badge bg-success font-size-12 ms-2">Paid</span></h5>
                                <div class="mb-4">
                                   <h2 class="tit"  style="font-size: 30px; color: #000;">Victor Online Shop</h2>
                                </div>
                                <div class="text-muted">
                                    <p class="mb-1">Calicut Kerala India</p>
                                    <p class="mb-1"><i class="uil uil-envelope-alt me-1"></i> www.victoronlineshop.com</p>
                                    <p><i class="uil uil-phone me-1"></i> 7070-100007</p>
                                </div>
                            </div>
        
                            <hr class="my-4">
        
                            <div class="row">
                                <div class="col-sm-6">
                                    <div class="text-muted">
                                        <h5 class="font-size-16 mb-3">Billed To:</h5>
                                        <!-- Display user's billing information -->
                                        <p class="font-size-15 mb-2"> <%= orderDetails.orderUserDetails.name %> - <%= orderDetails.orderUserDetails.phone %></p>
                                            <p class="box-content">
                                              <%= userData.email %> <br>
                                               <%= orderDetails.orderUserDetails.streetaddress %> - <%= orderDetails.orderUserDetails.pincode %>
                                               <%= orderDetails.orderUserDetails.landmark %> <br>  <%= orderDetails.orderUserDetails.place %> , <%= orderDetails.orderUserDetails.country %>
                                               <br><%= orderDetails.orderUserDetails.locality %>
                                               <%= orderDetails.orderUserDetails.alternatePhone %>
                                            </p>
                                    </div>
                                </div>
                                
                                <!-- end col -->
                                <div class="col-sm-6">
                                    <div class="text-muted text-sm-end">
                                        <div>
                                            <h5 class="font-size-15 mb-1">Invoice No:</h5>
                                            <p>#DZ0112</p>
                                        </div>
                                        <div class="mt-4">
                                            <h5 class="font-size-15 mb-1">Invoice Date:</h5>
                                            <p><%= orderDetails.orderDate.toString().split(' ').slice(0,4).join(' ') %></p>
                                        </div>
                                        <div class="mt-4">
                                            <h5 class="font-size-15 mb-1">Order No:</h5>
                                            <p>#<%= orderDetails._id %></p>
                                        </div>
                                    </div>
                                </div>
                                <!-- end col -->
                            </div>
                            <!-- end row -->
                            
                            <div class="py-2">
                                <h5 class="font-size-15">Order Summary</h5>
        
                                <div class="table-responsive">
                                    <table class="table align-middle table-nowrap table-centered mb-0">
                                        <thead>
                                            <tr>
                                                <th style="width: 70px; color: black;">No.</th>
                                                <th style="color: black;">Item</th>
                                                <th style="color: black;">Price</th>
                                                <th style="color: black;">Quantity</th>
                                                <th class="text-end" style="width: 120px; color: black;">Total</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <% orderDetails.products.forEach((product, index) => { %>
                                                <tr>
                                                    <td><%= index + 1 %></td>
                                                    <td>
                                                        <div>
                                                            <h5 style="font-size: 15px; font-weight: bold;" class="text-truncate font-size-14 mb-1"><%= product.productId.name %></h5>
                                                            <!-- <p class="text-muted mb-0"><%= product.productId.description %></p> -->
                                                        </div>
                                                    </td>
                                                    <!-- Display product price, quantity, and total -->
                                                    <td style="font-size: 15px; font-weight: bold;">₹ <%= product.productId.price %></td>
                                                    <td style="font-size: 15px; font-weight: bold;"><%= product.quantity %></td>
                                                    <td style="font-size: 15px; font-weight: bold;" class="text-end">₹ <%= product.productId.price * product.quantity %></td>
                                                </tr>
                                                <!-- end loop -->
                                            <!-- end tr -->
                                           
                                            <!-- end tr -->
                                            <tr>
                                                <th  style="font-size: 15px; font-weight: 400; color: #000;"  scope="row" colspan="4" class="text-end">Sub Total</th>
                                                <td  style="font-size: 15px; font-weight: 400;" class="text-end">₹<%= product.productId.price * product.quantity %></td>
                                            </tr>
                                            <!-- end tr -->
                                            <tr>
                                                <th  style="font-size: 15px; font-weight: 400; color: #000;" scope="row" colspan="4" class="border-0 text-end">
                                                    Discount :</th>
                                                <td style="font-size: 15px; font-weight: 400;" class="border-0 text-end">- ₹<%= product.productId.price - product.productId.offerPrice %></td>
                                            </tr>
                                            <!-- end tr -->
                                            <tr>
                                                <th  style="font-size: 15px; font-weight: 400; color: #000;" scope="row" colspan="4" class="border-0 text-end">
                                                    Shipping Charge :</th>
                                                <td  style="font-size: 15px; font-weight: 400;" class="border-0 text-end">₹00.00</td>
                                            </tr>
                                            <!-- end tr -->
                                            <tr>
                                                <th  style="font-size: 15px; font-weight: 400; color: #000;" scope="row" colspan="4" class="border-0 text-end">
                                                    Tax:</th>
                                                <td  style="font-size: 15px; font-weight: 400;" class="border-0 text-end">₹00.00</td>
                                            </tr>
                                            <!-- end tr -->
                                            <tr>
                                                <th  style="font-size: 15px; font-weight: 400; color: #000;" scope="row" colspan="4" class="border-0 text-end">Total</th>
                                                <td  style="font-size: 15px; font-weight: 400;" class="border-0 text-end"><h4  style="font-size: 15px; font-weight: bold;" class="m-0 fw-semibold">₹<%= orderDetails.totalAmount %></h4></td>
                                            </tr>
                                            <% }) %>

                                            <!-- end tr -->
                                        </tbody><!-- end tbody -->
                                    </table><!-- end table -->
                                </div><!-- end table responsive -->
                                <div class="d-print-none mt-4">
                                    <div class="float-end">
                                        <a href="javascript:window.print()" class="btn btn-success me-1"><i class="fa fa-print"></i></a>
                                        <!-- <a href="#" class="btn btn-primary w-md">Send</a> -->
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div><!-- end col -->
            </div>
        </div>







